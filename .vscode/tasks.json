{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "make clean build",
      "type": "shell",
      "command": "make",
      "args": [
        "clean",
        "build"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "make run",
      "type": "shell",
      "command": "make",
      "args": [
        "run"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}